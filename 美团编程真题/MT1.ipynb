    "# -*- coding:utf-8 -*-\n",
    "\n",
    "class LongestDistance:\n",
    "    def getDis(self, A, n):\n",
    "        # write code here\n",
    "        dis = 0\n",
    "        for i in range(len(A) - 1):\n",
    "            if max(A[i+1:]) - A[i] > dis:\n",
    "                dis = max(A[i+1:]) - A[i]\n",
    "        return dis"
