   "source": [
    "A, B = map(int,input().split())\n",
    "\n",
    "def gcd(A, B):\n",
    "    while A % B != 0:\n",
    "        A, B = B, A % B\n",
    "    return B\n",
    "\n",
    "print(int (A * B / gcd(A,B)))"
   ]
  }
